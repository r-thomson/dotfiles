# Partial .gitconfig with some tweaks and aliases

[pager]
	branch = false
	stash = false
	tag = false
[core]
	pager = delta
	excludesfile = ~/.gitignore
[alias]
	olog = log --oneline
	stat = diff --stat
	shortstat = diff --shortstat
	fixup = commit --amend --no-edit
	ff = merge --ff-only
[pull]
	ff = only
[init]
	defaultBranch = master
